{
	"name": "intera-typo3-extension/webkitpdf",
	"description": "Generate PDF files using WebKit rendering engine.",
	"type": "typo3-cms-extension",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Dev-Team Typoheads",
			"email": "dev.deleteme@typoheads.at"
		},
		{
			"name": "Alexander Stehlik",
			"email": "alexander.stehlik.deleteme@gmail.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Tx\\Webkitpdf\\": "Classes/"
		}
	},
	"require": {
		"php": "^7.0",
		"typo3/cms-core": "^7.6|^8.7|^9.3"
	},
	"replace": {
		"typo3-ter/webkitpdf": "self.version"
	},
	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin"
	},
	"scripts": {
		"post-autoload-dump": [
			"mkdir -p .Build/Web/typo3conf/ext/",
			"[ -L .Build/Web/typo3conf/ext/webkitpdf ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/webkitpdf"
		]
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": ".Build/Web"
		}
	}
}
